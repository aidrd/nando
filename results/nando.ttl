    rdfs:seeAlso <https://www.shouman.jp/disease/details/04_19_023/> ;
    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_2100061> ;
    skos:closeMatch obo:MONDO_0021209 ;
    skos:prefLabel "心臓腫瘍"@ja .

