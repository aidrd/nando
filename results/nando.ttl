    rdfs:seeAlso <https://www.kegg.jp/dbget-bin/www_bget?ds_ja:H01309>, <https://www.mhlw.go.jp/file/06-Seisakujouhou-10900000-Kenkoukyoku/0000157708.docx>, <https://www.nanbyou.or.jp/entry/266> ;
    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_1100010> ;
    skos:closeMatch obo:MONDO_0019338 ;
    skos:prefLabel "サルコイドーシス"@ja .

