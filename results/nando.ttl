    rdfs:seeAlso <https://www.shouman.jp/disease/details/11_37_096/> ;
    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_2100248> ;
    skos:closeMatch obo:MONDO_0020640 ;
    skos:prefLabel "自己免疫介在性脳炎・脳症"@ja .

