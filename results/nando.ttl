    rdfs:seeAlso <http://nanbyodata.jp/ontology/NANDO_2201436>, <https://www.nanbyou.or.jp/entry/22459>, <https://www.nanbyou.or.jp/wp-content/uploads/upload_files/File/338-202111-kijyun.pdf> ;
    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_1201042> ;
