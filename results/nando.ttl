    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_2100015> ;
    skos:altLabel "Juvenile nephronophthisis"@en, "NPH"@en ;
    skos:closeMatch obo:MONDO_0009728, obo:MONDO_0019005 ;
    skos:prefLabel "ネフロン癆"@ja .

