    a owl:Class ;
    rdfs:label "Supramitral ring"@en, "そうぼうべんべんじょうりん"@ja-Hira, "僧帽弁弁上輪"@ja ;
    rdfs:seeAlso <https://www.shouman.jp/disease/details/04_64_095/> ;
