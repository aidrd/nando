    rdfs:seeAlso <http://nanbyodata.jp/ontology/NANDO_2200832>, <https://www.kegg.jp/dbget-bin/www_bget?ds_ja:H00076>, <https://www.mhlw.go.jp/file/06-Seisakujouhou-10900000-Kenkoukyoku/0000157960.docx>, <https://www.nanbyou.or.jp/entry/4436> ;
    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_1100014> ;
    skos:altLabel "Cockayne症候群"@ja ;
    skos:closeMatch obo:MONDO_0016006 ;
    skos:prefLabel "コケイン症候群"@ja .

