    rdfs:seeAlso <https://www.mhlw.go.jp/file/06-Seisakujouhou-10900000-Kenkoukyoku/0000198077.docx>, <https://www.nanbyou.or.jp/entry/4553> ;
    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_1100001> ;
    skos:altLabel "Cerebro-oculo-hepato-renal syndrome"@en ;
    skos:closeMatch obo:MONDO_0015369 ;
    skos:prefLabel "ジュベール疾患群関連疾患"@ja .

