    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_1100009> ;
    skos:altLabel "Pituitary ADH secretion disorders"@en, "SIADH"@en, "Syndrome of inappropriate ADH secretion"@en ;
    skos:prefLabel "下垂体性ADH分泌異常症"@ja .

