    rdfs:seeAlso <https://www.shouman.jp/disease/details/02_03_023/> ;
    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_2100011> ;
    skos:altLabel "TIN"@en ;
    skos:closeMatch obo:MONDO_0001085 ;
    skos:prefLabel "慢性尿細管間質性腎炎"@ja .

