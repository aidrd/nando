    rdfs:seeAlso <http://syndromefinder.ncchd.go.jp/UR-DBMS/SyndromeDetail.php?winid=1&recid=688>, <https://www.kegg.jp/dbget-bin/www_bget?ds_ja:H00081>, <https://www.shouman.jp/disease/details/05_11_021/> ;
    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_2100120> ;
    skos:altLabel "Hashimoto’s thyroiditis"@en ;
    skos:closeMatch obo:MONDO_0007699 ;
    skos:prefLabel "橋本病"@ja .

