    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_2100160> ;
    skos:altLabel "Tyrosinaemia type 1"@en ;
    skos:closeMatch obo:MONDO_0010161 ;
    skos:prefLabel "高チロシン血症１型"@ja .

