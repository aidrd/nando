    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_1100002> ;
    skos:altLabel "Hepatic glycogenosis"@en ;
    skos:closeMatch obo:MONDO_0002412, obo:MONDO_0009414 ;
    skos:prefLabel "肝型糖原病"@ja .

