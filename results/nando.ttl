    rdfs:seeAlso <https://www.shouman.jp/disease/details/05_04_005/> ;
    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_2100113> ;
    skos:altLabel "GH分泌不全性低身長症（脳の器質的原因によるものに限る。）"@ja ;
