    rdfs:seeAlso <http://syndromefinder.ncchd.go.jp/UR-DBMS/SyndromeDetail.php?winid=1&recid=2266>, <https://www.shouman.jp/disease/details/08_03_048/> ;
    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_2100162> ;
    skos:altLabel "HADH deficiency"@en ;
    skos:closeMatch obo:MONDO_0009278 ;
    skos:prefLabel "3-ヒドロキシアシルCoA脱水素酵素欠損症"@ja .

