    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_2100179> ;
    skos:altLabel "Sideroblastic anaemia"@en ;
    skos:closeMatch obo:MONDO_0015194 ;
    skos:prefLabel "鉄芽球性貧血"@ja .

