    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_2100156> ;
    skos:altLabel "HIDS"@en, "MKD"@en, "Mevalonate kinase deficiency"@en, "メバロン酸キナーゼ欠損症"@ja ;
    skos:closeMatch obo:MONDO_0009849, obo:MONDO_0017708 ;
