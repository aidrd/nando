    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_1100005> ;
    skos:altLabel "PA sling"@en, "Pulmonary artery sling"@en ;
    skos:prefLabel "左肺動脈右肺動脈起始症"@ja .

