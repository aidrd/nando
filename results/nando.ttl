    rdfs:seeAlso <https://www.mhlw.go.jp/file/06-Seisakujouhou-10900000-Kenkoukyoku/0000157666.docx>, <https://www.nanbyou.or.jp/entry/205> ;
    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_1100004> ;
