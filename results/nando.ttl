    rdfs:label "NLRC4 mutation"@en, "NLRC４異常症"@ja, "えぬえるあーるしー4いじょうしょう"@ja-Hira ;
    rdfs:seeAlso <http://nanbyodata.jp/ontology/NANDO_1200994>, <https://www.shouman.jp/disease/details/06_05_024/> ;
