    rdfs:seeAlso <http://nanbyodata.jp/ontology/NANDO_2200903>, <https://www.kegg.jp/dbget-bin/www_bget?ds_ja:H01829>, <https://www.mhlw.go.jp/file/06-Seisakujouhou-10900000-Kenkoukyoku/0000157258.docx>, <https://www.nanbyou.or.jp/entry/4460> ;
    rdfs:subClassOf <http://nanbyodata.jp/ontology/NANDO_1100001> ;
    skos:altLabel "AERRPS"@en, "Refractory frequent partial seizures intussusception acute encephalitis"@en ;
