    <rdfs:label xml:lang="ja-Hira">ぐるたるさんけっしょう2がた</rdfs:label>
    <rdfs:label xml:lang="ja">グルタル酸血症2型</rdfs:label>
    <rdfs:seeAlso rdf:resource="http://nanbyodata.jp/ontology/NANDO_2200502"/>
    <rdfs:seeAlso rdf:resource="http://syndromefinder.ncchd.go.jp/UR-DBMS/SyndromeDetail.php?winid=1&amp;recid=2269"/>
    <rdfs:seeAlso rdf:resource="https://www.mhlw.go.jp/file/06-Seisakujouhou-10900000-Kenkoukyoku/0000157181.docx"/>
    <rdfs:seeAlso rdf:resource="https://www.nanbyou.or.jp/entry/4819"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_1100002"/>
