    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100168"/>
    <skos:prefLabel xml:lang="ja">114から118までに掲げるもののほか、プリンピリミジン代謝異常症</skos:prefLabel>
  </owl:Class>
