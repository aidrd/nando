    <rdfs:seeAlso rdf:resource="https://www.shouman.jp/disease/details/11_31_074/"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100245"/>
    <skos:altLabel xml:lang="en">SSPE</skos:altLabel>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0009835"/>
    <skos:prefLabel xml:lang="ja">亜急性硬化性全脳炎</skos:prefLabel>
