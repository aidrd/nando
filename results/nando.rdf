    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_1100004"/>
    <skos:altLabel xml:lang="en">hereditary autoinflammatory diseases</skos:altLabel>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0019751"/>
    <skos:prefLabel xml:lang="ja">遺伝性自己炎症疾患</skos:prefLabel>
