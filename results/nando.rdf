    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100146"/>
    <skos:altLabel xml:lang="en">Primary hypophosphataemic rickets</skos:altLabel>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0024300"/>
    <skos:prefLabel xml:lang="ja">原発性低リン血症性くる病</skos:prefLabel>
  </owl:Class>
