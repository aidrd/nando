    <rdfs:seeAlso rdf:resource="https://www.mhlw.go.jp/file/06-Seisakujouhou-10900000-Kenkoukyoku/0000157117.docx"/>
    <rdfs:seeAlso rdf:resource="https://www.nanbyou.or.jp/entry/4844"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_1100014"/>
    <skos:altLabel xml:lang="en">AGS</skos:altLabel>
    <skos:altLabel xml:lang="ja">Alagille症候群</skos:altLabel>
