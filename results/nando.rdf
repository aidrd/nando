    <rdfs:seeAlso rdf:resource="https://www.shouman.jp/disease/details/16_01_004/"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100295"/>
    <skos:altLabel xml:lang="ja">Klippel-Trenanay-Weber症候群</skos:altLabel>
    <skos:altLabel xml:lang="en">Klippel–Trénaunay syndrome</skos:altLabel>
