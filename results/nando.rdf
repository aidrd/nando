    <rdfs:seeAlso rdf:resource="https://www.nanbyou.or.jp/entry/241"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_1100001"/>
    <skos:altLabel xml:lang="en">POEMS syndrome</skos:altLabel>
    <skos:altLabel xml:lang="ja">POEMS症候群</skos:altLabel>
