    <rdfs:seeAlso rdf:resource="https://www.shouman.jp/disease/details/04_20_024/"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100062"/>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0004496"/>
