    <rdfs:seeAlso rdf:resource="https://www.shouman.jp/disease/details/04_38_047/"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100080"/>
    <skos:altLabel xml:lang="ja">Ebstein病</skos:altLabel>
