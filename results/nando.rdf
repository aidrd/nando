    <rdfs:seeAlso rdf:resource="http://syndromefinder.ncchd.go.jp/UR-DBMS/SyndromeDetail.php?winid=1&amp;recid=2400"/>
    <rdfs:seeAlso rdf:resource="https://www.kegg.jp/dbget-bin/www_bget?ds_ja:H01598"/>
    <rdfs:seeAlso rdf:resource="https://www.mhlw.go.jp/file/06-Seisakujouhou-10900000-Kenkoukyoku/0000157667.docx"/>
    <rdfs:seeAlso rdf:resource="https://www.nanbyou.or.jp/entry/206"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_1100009"/>
    <skos:altLabel xml:lang="en">Addison disease</skos:altLabel>
    <skos:altLabel xml:lang="ja">Addison病</skos:altLabel>
    <skos:altLabel xml:lang="en">Primary chronic adrenocortical insufficiency</skos:altLabel>
