    <rdfs:seeAlso rdf:resource="https://www.shouman.jp/disease/details/11_21_056/"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100235"/>
    <skos:altLabel xml:lang="en">SJS</skos:altLabel>
    <skos:altLabel xml:lang="ja">Schwartz-Jampel症候群</skos:altLabel>
