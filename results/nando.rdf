    <rdfs:seeAlso rdf:resource="https://www.shouman.jp/disease/details/06_01_002/"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100152"/>
    <skos:altLabel xml:lang="en">SLE</skos:altLabel>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0007915"/>
    <skos:prefLabel xml:lang="ja">全身性エリテマトーデス</skos:prefLabel>
