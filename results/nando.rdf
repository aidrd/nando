    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100163"/>
    <skos:prefLabel xml:lang="ja">50から57までに掲げるもののほか、ミトコンドリア病</skos:prefLabel>
  </owl:Class>
