    <rdfs:seeAlso rdf:resource="https://www.nanbyou.or.jp/entry/4825"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_1100002"/>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0015454"/>
    <skos:prefLabel xml:lang="ja">複合カルボキシラーゼ欠損症</skos:prefLabel>
