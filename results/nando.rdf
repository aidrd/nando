    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0006987"/>
    <skos:prefLabel xml:lang="ja">大動脈弁下狭窄症</skos:prefLabel>
  </owl:Class>
  <owl:Class rdf:about="http://nanbyodata.jp/ontology/NANDO_2200278">
