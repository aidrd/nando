    <rdfs:label xml:lang="ja">先天性フィブリノーゲン欠乏症</rdfs:label>
    <rdfs:seeAlso rdf:resource="https://www.shouman.jp/disease/details/09_21_036/"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100196"/>
    <skos:altLabel xml:lang="en">Afibrinogenaemia</skos:altLabel>
    <skos:prefLabel xml:lang="ja">先天性フィブリノーゲン欠乏症</skos:prefLabel>
  </owl:Class>
