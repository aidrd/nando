    <rdfs:seeAlso rdf:resource="http://nanbyodata.jp/ontology/NANDO_2200841"/>
    <rdfs:seeAlso rdf:resource="http://syndromefinder.ncchd.go.jp/UR-DBMS/SyndromeDetail.php?winid=1&amp;recid=6748"/>
