    <rdfs:seeAlso rdf:resource="http://syndromefinder.ncchd.go.jp/UR-DBMS/SyndromeDetail.php?winid=1&amp;recid=586"/>
    <rdfs:seeAlso rdf:resource="https://www.shouman.jp/disease/details/09_21_040/"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100196"/>
    <skos:altLabel xml:lang="en">Haemophilia A</skos:altLabel>
