    <rdfs:label xml:lang="ja">ＴＲＰＶ４異常症</rdfs:label>
    <rdfs:seeAlso rdf:resource="https://www.shouman.jp/disease/details/15_02_014/"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100293"/>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0018240"/>
    <skos:prefLabel xml:lang="ja">ＴＲＰＶ４異常症</skos:prefLabel>
