    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100225"/>
    <skos:altLabel xml:lang="en">IGD</skos:altLabel>
    <skos:altLabel xml:lang="ja">先天性GPI欠損症</skos:altLabel>
