    <skos:altLabel xml:lang="en">LCH</skos:altLabel>
    <skos:altLabel xml:lang="ja">Langerhans細胞組織球症</skos:altLabel>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0017025"/>
