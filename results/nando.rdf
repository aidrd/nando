    <rdfs:label xml:lang="ja-Hira">げんぱつせいこうかいろみくろんけっしょう</rdfs:label>
    <rdfs:label xml:lang="ja">原発性高カイロミクロン血症</rdfs:label>
    <rdfs:seeAlso rdf:resource="http://nanbyodata.jp/ontology/NANDO_2200601"/>
    <rdfs:seeAlso rdf:resource="https://www.kegg.jp/dbget-bin/www_bget?ds_ja:H01784"/>
