    <rdfs:seeAlso rdf:resource="https://www.shouman.jp/disease/details/15_02_002/"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100293"/>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0007037"/>
    <skos:prefLabel xml:lang="ja">軟骨無形成症</skos:prefLabel>
