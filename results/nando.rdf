    <rdfs:label xml:lang="ja">急性肝不全（昏睡型）</rdfs:label>
    <rdfs:seeAlso rdf:resource="https://www.shouman.jp/disease/details/12_06_019/"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100262"/>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0001548"/>
    <skos:prefLabel xml:lang="ja">急性肝不全（昏睡型）</skos:prefLabel>
  </owl:Class>
  <owl:Class rdf:about="http://nanbyodata.jp/ontology/NANDO_2200927">
