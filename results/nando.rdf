    <rdfs:seeAlso rdf:resource="https://www.nanbyou.or.jp/entry/226"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_1100002"/>
    <skos:altLabel xml:lang="ja">FHホモ接合体</skos:altLabel>
    <skos:altLabel xml:lang="en">Homozygous FH</skos:altLabel>
