    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_1100005"/>
    <skos:altLabel xml:lang="en">Univentricular heart</skos:altLabel>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0015451"/>
