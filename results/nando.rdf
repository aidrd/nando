    <rdfs:seeAlso rdf:resource="https://www.kegg.jp/dbget-bin/www_bget?ds_ja:H01803"/>
    <rdfs:seeAlso rdf:resource="https://www.mhlw.go.jp/file/06-Seisakujouhou-10900000-Kenkoukyoku/0000157216.docx"/>
    <rdfs:seeAlso rdf:resource="https://www.nanbyou.or.jp/entry/4876"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_1100005"/>
    <skos:altLabel xml:lang="ja">極型ファロー四徴症</skos:altLabel>
