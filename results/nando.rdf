    <rdfs:seeAlso rdf:resource="https://www.kegg.jp/dbget-bin/www_bget?ds_ja:H00842"/>
    <rdfs:seeAlso rdf:resource="https://www.shouman.jp/disease/details/10_06_048/"/>
