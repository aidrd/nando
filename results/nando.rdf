    <rdfs:seeAlso rdf:resource="https://www.mhlw.go.jp/file/06-Seisakujouhou-10900000-Kenkoukyoku/0000157258.docx"/>
    <rdfs:seeAlso rdf:resource="https://www.nanbyou.or.jp/entry/4459"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_1100001"/>
    <skos:altLabel xml:lang="en">Refractory frequent partial seizures intussusception acute encephalitis</skos:altLabel>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0015584"/>
