    <rdfs:seeAlso rdf:resource="https://www.nanbyou.or.jp/entry/4714"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_1100008"/>
    <skos:altLabel xml:lang="en">TD</skos:altLabel>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0017042"/>
