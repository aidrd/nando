    <rdfs:seeAlso rdf:resource="https://www.nanbyou.or.jp/entry/291"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_1100001"/>
    <skos:altLabel xml:lang="en">CBD</skos:altLabel>
