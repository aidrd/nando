    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100150"/>
    <skos:altLabel xml:lang="ja">Turner症候群</skos:altLabel>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0019499"/>
    <skos:prefLabel xml:lang="ja">ターナー症候群</skos:prefLabel>
  </owl:Class>
