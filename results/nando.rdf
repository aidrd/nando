    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_2100219"/>
    <skos:altLabel xml:lang="ja">Rett症候群</skos:altLabel>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0010726"/>
    <skos:prefLabel xml:lang="ja">レット症候群</skos:prefLabel>
