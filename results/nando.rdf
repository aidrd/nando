    <rdfs:label xml:lang="ja-Hira">じゅうしょうせんてんせいこうちゅうきゅうげんしょうしょう</rdfs:label>
    <rdfs:label xml:lang="ja">重症先天性好中球減少症</rdfs:label>
    <rdfs:seeAlso rdf:resource="http://nanbyodata.jp/ontology/NANDO_1200353"/>
    <rdfs:seeAlso rdf:resource="https://www.shouman.jp/disease/details/10_05_035/"/>
