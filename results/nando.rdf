    <rdfs:seeAlso rdf:resource="https://www.mhlw.go.jp/file/06-Seisakujouhou-10900000-Kenkoukyoku/0000157195.docx"/>
    <rdfs:seeAlso rdf:resource="https://www.nanbyou.or.jp/entry/4567"/>
