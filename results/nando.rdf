    <rdfs:label xml:lang="ja-Hira">きんじすとろふぃー</rdfs:label>
    <rdfs:label xml:lang="ja">筋ジストロフィー</rdfs:label>
    <rdfs:seeAlso rdf:resource="https://www.mhlw.go.jp/file/06-Seisakujouhou-10900000-Kenkoukyoku/0000157176.docx"/>
    <rdfs:seeAlso rdf:resource="https://www.nanbyou.or.jp/entry/4523"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_1100001"/>
    <skos:closeMatch rdf:resource="http://purl.obolibrary.org/obo/MONDO_0020121"/>
    <skos:prefLabel xml:lang="ja">筋ジストロフィー</skos:prefLabel>
  </owl:Class>
