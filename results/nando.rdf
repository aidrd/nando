    <rdfs:seeAlso rdf:resource="https://www.mhlw.go.jp/file/06-Seisakujouhou-10900000-Kenkoukyoku/0000198069.docx"/>
    <rdfs:seeAlso rdf:resource="https://www.nanbyou.or.jp/entry/317"/>
    <rdfs:subClassOf rdf:resource="http://nanbyodata.jp/ontology/NANDO_1100013"/>
